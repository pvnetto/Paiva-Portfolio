{
  "name": "paiva-portfolio",
  "version": "1.0.0",
  "description": "Paiva's Portfolio website",
  "homepage": "https://pvnetto.github.io/Paiva-Portfolio/",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "webpack-dev-server --open",
    "start": "node server.js",
    "build": "webpack --config webpack.config.prod.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "heroku-postbuild": "npm run build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "<rootDir>/src/tests/enzyme-setup.js"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|pdf)$": "<rootDir>/src/tests/__mocks__/file-mock.js"
    }
  },
  "engines": {
    "node": "10.15.0"
  },
  "keywords": [
    "paiva",
    "pvnetto",
    "portfolio",
    "html",
    "css",
    "3d",
    "three",
    "javascript",
    "react",
    "unity3d",
    "front-end"
  ],
  "author": "Paiva",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.1",
    "@react-spring/core": "^9.7.3",
    "@react-spring/three": "^9.7.3",
    "@react-spring/web": "^9.7.3",
    "@react-three/fiber": "^8.16.6",
    "@react-three/postprocessing": "^2.16.2",
    "@use-gesture/react": "^10.3.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "emailjs-com": "^2.6.4",
    "express": "^4.19.2",
    "formik": "^2.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "react-spring": "^9.7.3",
    "three": "^0.164.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-modules-commonjs": "^7.24.1",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@testing-library/react": "^15.0.7",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-require-context-hook": "^1.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-webpack-plugin": "^4.1.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^6.1.1",
    "html-webpack-plugin": "^5.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "source-map-explorer": "^2.5.3",
